{
  "$schema": "node_modules/wrangler/config-schema.json",
  "name": "blog-app",
  "main": "src/worker.tsx",
  "compatibility_date": "2025-04-01",
  "compatibility_flags": ["nodejs_compat"],
  "assets": {
    "binding": "ASSETS"
  },
  "observability": {
    "enabled": true
  },
  "d1_databases": [
    {
      "binding": "DB",
      "database_name": "blog-db",
      "database_id": "local"
    }
  ]
}
